{

    "manifest_version": 2,
    "name": "hentaiblocker",
    "version":"1.0",

    "description": "Test program to block certain websites",

    "icons": {
      "48": "icons/nnn-48.png"
    },

    "content_scripts": [
        {
        "matches": ["*://nhentai.net/*"],
        "js": ["hentaiblocker.js"]
        }
    ],

    "background": {
      "scripts": ["background.js"]
    },

    "permissions": [
      "<all_urls>", "webRequest", "webRequestBlocking", "storage", "tabs", "idle"
  ]

}